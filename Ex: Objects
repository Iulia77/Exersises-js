//Ex: 1

let salaries = {
    'John': 100,
    'Pete': 300,
    "Mary": 250
};

alert(sumSalaries(salaries));

const sumSalaries = salaries => {
    let sum = 0;
    for (let salary of Object.values(salaries)) {
        sum += salary;
    }
    return sum;
}


//Ex: 2

let user = {
    name: 'John',
    age: 30
};

alert(count(user));

function count(obj) {
    return Object.keys(obj).length;
};


